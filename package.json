    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "cypress": "^9.4.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.8.0",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "postcss": "8.2.13",
    "nth-check": "2.0.1",
    "css-what": "5.0.1"
  }
}
